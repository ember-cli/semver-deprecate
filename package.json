    "@eslint/js": "^9.17.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "fixturify-project": "^7.1.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rollup": "^4.30.0",
    "strip-ansi": "^7.1.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "semver": "^7.6.3"
  },
